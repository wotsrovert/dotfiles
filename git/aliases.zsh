alias gb='git branch'
alias gbx='git branch | grep -v _x_'
alias gcm='git commit -m'
alias gcma='git commit --amend'
alias gcmwip='git commit -m "w.i.p."'
alias gcom='git checkout master'
alias gd='git diff'
alias gdc='git diff --cached'
alias gdm='git diff master'
alias gdn='git diff --name-only'
alias gdnm='git diff --name-only master'
alias glom='git pull origin master --rebase'
alias gmm='git merge master'
alias gpo='git push origin'
alias grbm="git rebase master"
alias grc='git rebase --continue'
alias grk='git rebase --skip'
alias grm="git reset `git log --oneline master | head -n 1 | awk -F' ' '{print $1}'`"
alias grs='git restore --staged'
alias gs='git status'

alias gdnms='gdnm | grep _spec.rb'
alias gclbr='gcom && glom && git branch --merged'
alias gbxg='gbx | grep'
